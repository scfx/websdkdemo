    { type: NgModule, args: [{
                imports: [
                    CommonModule$1,
                    ReactiveFormsModule,
                    I18nModule,
                    ValidationModule,
                    DynamicFormsUIThemeModule,
                    FormlyModule.forRoot()
                ],
