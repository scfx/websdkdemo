import { Renderer2, ElementRef, OnDestroy, AfterContentInit } from '@angular/core';
import { Router } from '@angular/router';
import { Tab } from './tab.model';
import { HeaderService } from '../header/header.service';
export declare class TabsOutletComponent implements OnDestroy, AfterContentInit {
    private router;
    private renderer;
    private el;
    private header;
    tabs: any[];
    orientation: 'vertical' | 'horizontal';
    navigatorOpen: boolean;
    showLeft: boolean;
    showRight: boolean;
    get isNavigatorOpen(): boolean;
    get hasTabs(): number;
    get isHorizontal(): boolean;
    get isVertical(): boolean;
    private container;
    private leftBtn;
    private rightBtn;
    private tabsContainer;
    private scrollSubscription;
    constructor(router: Router, renderer: Renderer2, el: ElementRef, header: HeaderService);
    ngOnDestroy(): void;
    ngAfterContentInit(): void;
    isActive(tab: Tab): boolean;
    navigateTo(path: any): void;
    scroll(direction?: 'right' | 'left'): void;
    private scrollSubscribe;
    private updateScroll;
    private getDimensions;
}
