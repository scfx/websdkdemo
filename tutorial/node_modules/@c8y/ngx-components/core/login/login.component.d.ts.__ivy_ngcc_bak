import { OnInit } from '@angular/core';
import { ICredentials } from '@c8y/client';
import { LoginService } from './login.service';
import { OptionsService } from '../common/options.service';
import { LoginViews } from './login.model';
import { AlertService } from '../alert/alert.service';
export declare class LoginComponent implements OnInit {
    loginService: LoginService;
    private options;
    private alert;
    currentView: LoginViews;
    LOGIN_VIEWS: typeof LoginViews;
    disabled: boolean;
    name: string;
    credentials: ICredentials;
    displayAlerts: boolean;
    private TOKEN_PARAM;
    /**
     * Just DI.
     */
    constructor(loginService: LoginService, options: OptionsService, alert: AlertService);
    ngOnInit(): void;
    handleLoginTemplate(event: any): void;
    onkeyup(event: KeyboardEvent): void;
    reset(): void;
    private loginAutomatically;
    private setView;
    private getResetPasswordToken;
}
