import { EventEmitter, OnDestroy, OnInit } from '@angular/core';
import { DeviceGridComponent } from '@c8y/ngx-components/device-grid';
import { Column } from '@c8y/ngx-components';
import { Observable } from 'rxjs';
export declare class DeviceSelectorComponent implements OnInit, OnDestroy {
    deviceGrid: DeviceGridComponent;
    deviceTypes: Observable<string[]>;
    typeColumn: Column;
    onDeviceQueryStringChange: EventEmitter<string>;
    columns: Column[];
    private unsubscribe$;
    ngOnInit(): void;
    ngOnDestroy(): void;
}
