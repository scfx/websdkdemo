import { OnInit } from '@angular/core';
import { Router } from '@angular/router';
import { IOperation, OperationService, OperationStatus } from '@c8y/client';
import { AlertService, ListItemComponent, Tab } from '@c8y/ngx-components';
export declare class SingleOperationListItemComponent implements OnInit {
    private router;
    private operationService;
    private alertService;
    operation: Partial<IOperation>;
    collapsed: boolean;
    readOnly: boolean;
    listItem: ListItemComponent;
    detailsTabTemplate: any;
    historyOfChangesTabTemplate: any;
    tabs: Tab[];
    selectedTab: Tab;
    statusIcons: {
        [x: number]: string;
    };
    statusClasses: {
        [x: number]: string;
    };
    OperationStatus: typeof OperationStatus;
    get displayedDescription(): any;
    constructor(router: Router, operationService: OperationService, alertService: AlertService);
    ngOnInit(): void;
    scheduleAsBulkOperation(): void;
    cancel(): Promise<void>;
}
