import { FetchClient, IFetchResponse } from '@c8y/client';
import { ImpactOptions } from './impact.model';
export declare class ImpactConnectivityService {
    private client;
    private readonly microserviceUrl;
    private readonly header;
    constructor(client: FetchClient);
    getOptions(): Promise<IFetchResponse>;
    updateOptions(impactOptions: ImpactOptions): Promise<IFetchResponse>;
    deleteOptions(): Promise<IFetchResponse>;
}
