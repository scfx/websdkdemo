import { ActivatedRouteSnapshot, CanActivate, RouterStateSnapshot } from '@angular/router';
import { ApplicationService } from '@c8y/client';
import { Permissions } from '@c8y/ngx-components';
export declare class SimGuard implements CanActivate {
    private permissions;
    private applicationService;
    private static readonly applicationName;
    private showSimProvider;
    constructor(permissions: Permissions, applicationService: ApplicationService);
    canActivate(route: ActivatedRouteSnapshot, state: RouterStateSnapshot): Promise<boolean>;
}
