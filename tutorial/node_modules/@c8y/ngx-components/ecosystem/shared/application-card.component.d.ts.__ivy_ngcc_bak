import { EventEmitter, OnInit } from '@angular/core';
import { Router } from '@angular/router';
import { ApplicationService, IApplication } from '@c8y/client';
import { AlertService } from '@c8y/ngx-components';
import { ApplicationState } from './../ecosystem.model';
import { EcosystemService } from './../ecosystem.service';
export declare class ApplicationCardComponent implements OnInit {
    private applicationService;
    private ecosystemService;
    private alertService;
    private router;
    app: IApplication;
    canEdit: boolean;
    onAppDeleted: EventEmitter<void>;
    appState: ApplicationState;
    canOpenInBrowser: boolean;
    disableOpenInBrowser: boolean;
    canDelete: boolean;
    isPackage: boolean;
    isFeature: boolean;
    isExternal: boolean;
    isMicroservice: boolean;
    get openButtonTitle(): any;
    constructor(applicationService: ApplicationService, ecosystemService: EcosystemService, alertService: AlertService, router: Router);
    ngOnInit(): Promise<void>;
    detail(): void;
    openApp(): void;
    delete(): Promise<void>;
}
