import { OnInit } from '@angular/core';
import { ModalService } from '@c8y/ngx-components';
import { ApplicationPlugin } from '../../ecosystem.model';
import { BehaviorSubject, Observable } from 'rxjs';
import { BsModalRef } from 'ngx-bootstrap/modal';
export declare class InstallPluginComponent implements OnInit {
    private bsModalRef;
    private modal;
    plugins$: BehaviorSubject<ApplicationPlugin[]>;
    filterTerm$: BehaviorSubject<string>;
    filteredPlugins$: Observable<ApplicationPlugin[]>;
    selectedPlugins: string[];
    result: Promise<void>;
    private _install;
    private _cancel;
    constructor(bsModalRef: BsModalRef, modal: ModalService);
    ngOnInit(): void;
    cancel(): void;
    install(): Promise<void>;
    private filterContainString;
}
