import { EventEmitter } from '@angular/core';
import { ISelectModalObject, ModalLabels } from '@c8y/ngx-components';
import { IManagedObject } from '@c8y/client';
import { Subject, Observable } from 'rxjs';
import { RepositoryService } from '@c8y/ngx-components/repository';
export declare class SelectConfigurationModalComponent {
    private repositoryService;
    title: string;
    load: Subject<void>;
    configurations: Observable<any[]>;
    resultEmitter: EventEmitter<any[]>;
    selected: any[];
    deviceTypeQuery: any;
    searchQuery: any;
    labels: ModalLabels;
    private queriesUtil;
    constructor(repositoryService: RepositoryService);
    search(searchTerm: any): void;
    result(selectedItems: any): void;
    getItems(): Promise<import("@c8y/client").IResultList<IManagedObject>>;
    aggregate(mos: IManagedObject[]): ISelectModalObject[];
}
