import { BsModalRef } from 'ngx-bootstrap/modal';
import { DroppedFile } from '@c8y/ngx-components';
import { ITrustedCertificate, TrustedCertificateService } from '@c8y/client';
import { AlertService } from '@c8y/ngx-components';
export declare class AddTrustedCertificateComponent {
    private modal;
    private trustedCertificateService;
    private alertService;
    maxAllowedFiles: number;
    AUTO_REGISTRATION_POPOVER: any;
    trustedCertificate: Partial<ITrustedCertificate>;
    fileIsUploaded: boolean;
    result: Promise<any>;
    private _save;
    private _cancel;
    constructor(modal: BsModalRef, trustedCertificateService: TrustedCertificateService, alertService: AlertService);
    uploadFile(dropped: DroppedFile[]): void;
    save(): Promise<void>;
    close(): void;
}
