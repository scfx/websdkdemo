import { OnInit } from '@angular/core';
import { ApplicationService, IApplication } from '@c8y/client';
import { WizardComponent } from '@c8y/ngx-components';
import { EcosystemService } from '../../ecosystem.service';
import { ApplicationPropertiesFormComponent } from '../../shared/application-properties-form.component';
export declare class InstallFromPackageComponent implements OnInit {
    private ecosystemService;
    private applicationService;
    private wizardComponent;
    newAppConfig: IApplication;
    selectedPackage: IApplication;
    packages: IApplication[];
    inProgress: boolean;
    deployedWithSuccess: boolean;
    isDeployed: boolean;
    applicationPropertiesForm: ApplicationPropertiesFormComponent;
    constructor(ecosystemService: EcosystemService, applicationService: ApplicationService, wizardComponent: WizardComponent);
    ngOnInit(): Promise<void>;
    back(): void;
    cancel(): void;
    save(): Promise<void>;
    selectPackage(selectedPackage: IApplication): Promise<void>;
    private markAsDeployed;
    private loadPackages;
}
