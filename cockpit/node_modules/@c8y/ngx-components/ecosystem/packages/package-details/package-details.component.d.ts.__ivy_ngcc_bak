import { OnInit } from '@angular/core';
import { ActivatedRoute } from '@angular/router';
import { FetchClient, IApplication } from '@c8y/client';
import { PropertiesListItem, WizardService } from '@c8y/ngx-components';
import { ApplicationState } from '../../ecosystem.model';
import { EcosystemService } from '../../ecosystem.service';
export declare class PackageDetailsComponent implements OnInit {
    private activatedRoute;
    private client;
    private wizard;
    private ecosystemService;
    readme: string;
    package: IApplication;
    name: string;
    description: string;
    appState: ApplicationState;
    isApplicationPackage: boolean;
    readonly packageProperties: PropertiesListItem[];
    private readonly headers;
    private NOT_FOUND_ERROR_CODE;
    constructor(activatedRoute: ActivatedRoute, client: FetchClient, wizard: WizardService, ecosystemService: EcosystemService);
    ngOnInit(): Promise<void>;
    deploy(): void;
    private getPackage;
    private getReadmeFileContentAsHtml;
    private getReadmeFile;
}
