import { EventEmitter, OnInit } from '@angular/core';
import { Permissions } from '@c8y/ngx-components';
import { SubAssetsService } from './sub-assets.service';
export declare class GroupsComponent implements OnInit {
    private permissions;
    private subAssetsService;
    refresh: EventEmitter<any>;
    showAddGroup: boolean;
    filterable: boolean;
    sortable: boolean;
    shouldDisableAddGroup: boolean;
    constructor(permissions: Permissions, subAssetsService: SubAssetsService);
    ngOnInit(): void;
}
