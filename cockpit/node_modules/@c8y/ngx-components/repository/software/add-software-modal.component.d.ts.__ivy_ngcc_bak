import { EventEmitter } from '@angular/core';
import { NgForm } from '@angular/forms';
import { BsModalRef } from 'ngx-bootstrap/modal';
import { RepositoryService } from '../repository.service';
import { ModalModel, RepositoryCategory } from '../repository.model';
import { AlertService } from '@c8y/ngx-components';
import { BehaviorSubject } from 'rxjs';
import { PickedFiles } from '@c8y/ngx-components';
import { IManagedObject } from '@c8y/client';
export declare class AddSoftwareModalComponent {
    private modal;
    private repositoryService;
    private alert;
    form: NgForm;
    saved: EventEmitter<RepositoryCategory>;
    onInput: BehaviorSubject<string>;
    model: ModalModel;
    softwaresResult: any;
    saving: boolean;
    softwarePreselected: boolean;
    textForSoftwareUrlPopover: string;
    private inputSubscription$;
    constructor(modal: BsModalRef, repositoryService: RepositoryService, alert: AlertService);
    ngOnInit(): void;
    setInitialState(): void;
    loadSoftwares(): void;
    getSoftwareResult(searchStr: string): import("rxjs").Observable<import("@c8y/client").IResultList<IManagedObject>>;
    save(): Promise<void>;
    successMsg(): void;
    cancel(): void;
    ngOnDestroy(): void;
    onFile(dropped: PickedFiles): void;
    onSoftwareSelect(software: IManagedObject): void;
}
