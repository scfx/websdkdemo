import { Directive, Input } from '@angular/core';
export class MessageDirective {
}
MessageDirective.decorators = [
    { type: Directive, args: [{
                selector: 'c8y-message',
            },] }
];
MessageDirective.propDecorators = {
    name: [{ type: Input }],
    text: [{ type: Input }]
};
