import { Injectable } from '@angular/core';
import { Realtime } from '@c8y/client';
import { RealtimeService } from './realtime.service';
/**
 * A service for handling bulk operation realtime notifications in RxJS fashion.
 */
export class OperationRealtimeService extends RealtimeService {
    constructor(realtime) {
        super(realtime);
    }
    channel() {
        return '/operations/*';
    }
}
OperationRealtimeService.decorators = [
    { type: Injectable }
];
OperationRealtimeService.ctorParameters = () => [
    { type: Realtime }
];
