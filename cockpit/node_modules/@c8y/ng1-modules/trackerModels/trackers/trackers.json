  {
    "name": "QUECLINK GL300",
    "description": "",
    "image": ":::PLUGIN_PATH:::/img/queclink-gl300.jpg",
    "IMEILength": 15,
    "supportedOperations": {
      "restart": false,
      "setSosNumber": false,
      "armAlarms": {
        "vibration": false,
        "noise": false,
        "door": false,
        "sos": false
      },
      "updateSituation": false,
      "getSingleLocation": false,
      "reportIntervalNoMotion": true,
      "reportIntervalOnMotion": true,
      "motionTracking": true,
      "registerBySMS": {
        "variables": {
          "trackerPassword": true,
          "userPhoneNumber": true,
          "trackerIMEI": true,
          "trackerPhoneNumber": true,
          "trackerAPN": true,
          "trackerDomainName": true
        },
        "defaults": {
          "trackerPassword": "gl300"
        },
        "messages": [
          {
            "requiresFeedback": true,
            "text": "'AT+GTQSS=' + tracker.trackerPassword + ',' + tracker.trackerAPN + ',,,3,,1,' + tracker.trackerDomainName + ',9090,,,,15,0,,,FFFF$'"
          }
        ]
      }
    }
